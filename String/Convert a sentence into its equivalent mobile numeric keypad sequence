#include <iostream>
#include<string>
using namespace std;

int main()
{
    string str[]={
        "2","22","222",
                    "3","33","333",
                    "4","44","444",
                    "5","55","555",
                    "6","66","666",
                    "7","77","777","7777",
                    "8","88","888",
                    "9","99","999","9999"
    };
    string s;
    cin>>s;
    string result;
    for(int i=0;i<s.size();i++){
        if(s[i]==' ')
          result+="0";
        else{
            int pos=s[i]-'a';
            result+=str[pos];
        }
    }
    for(int i=0;i<result.size();i++){
        cout<<result[i];
    }

    return 0;
}

A and a concept
