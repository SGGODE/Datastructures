void dfs(vector<int> adj,int node,vector<int>&v,int arr[]){
arr[node]=1;
v.push_back(node);
for(auto it:adj[node]){
  if(!arr[it]){
   dfs(adj,node,v,arr);
   }
 }
}
int main(){
 vector<int>adj;  //goven
 int node=0;
 vector<int> v;
 int arr[v]={0};
 //v is nuber of nodes/vertices
 dfs(adj,node,v,arr):
 return v;
 }
